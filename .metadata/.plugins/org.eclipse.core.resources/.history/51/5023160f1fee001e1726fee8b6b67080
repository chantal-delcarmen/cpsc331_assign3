package bst;

import java.util.Random;

public class Main {
	
	public static int generateRand(int maximum) {
		Random rand = new Random();
		int min = 0;
		int max = maximum;
		int randomNum = rand.nextInt(max - min + 1);		
		return randomNum;
	}
	
